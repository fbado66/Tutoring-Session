<div class='content'>
    <h1> New Coach Form</h1>

    <%= form_for @coach do |f| %>
    <%= f.label :name, "Name:" %>
    <%= f.text_field :name %> 
    <%= f.label :experience_years, "Years of Experience" %>
    <%= f.number_field :experience_years %><br><br>
    <%=f.submit "New Coach" %>
    <% end %>
</div>