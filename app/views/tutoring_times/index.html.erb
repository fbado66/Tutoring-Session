<div class='content'>
<h1> Tutoring Sessions List</h1>

    <p> <% @tutoring_times.map do |tutoring_info| %>
        
        Date: <%= link_to tutoring_info.appointment_time, tutoring_time_path(tutoring_info) %> <br>
        Coach: <%=tutoring_info.coach.name %> <br>
        Student: <%=tutoring_info.student.name %> <br>
        On <%=tutoring_info.student.language %> language <br>
    </p>
    <br>
    <% end %>
    <br><br>
    <span class='button'> <%=link_to 'GO BACK', "/" %></span>

</div>